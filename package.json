{
  "name": "socroxy",
  "description": "Socrates-proxy, a Secure Web Proxy.",
  "version": "1.8.13",
  "private": false,
  "homepage": "https://github.com/Leask/socrates",
  "main": "index.mjs",
  "type": "module",
  "bin": {
    "bistrot": "main.mjs"
  },
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "start": "node main.mjs",
    "debug": "node --inspect --trace-warnings main.mjs --http --debug",
    "test": "node --inspect --trace-warnings test.mjs",
    "updep": "npx ncu -u && npm install && wget https://raw.githubusercontent.com/Marak/colors.js/master/lib/styles.js -O ./lib/style.cjs",
    "gitsync": "( git commit -am \"Released @ `date`\" || true ) && git pull && git push",
    "build": "npm run updep && ( git commit -am 'update dependencies' || true ) && npm version patch",
    "prepublishOnly": "npm run build && npm run gitsync",
    "beta": "npm publish --tag beta"
  },
  "keywords": [
    "http-proxy",
    "mitm",
    "proxy",
    "secure",
    "tcp",
    "transparent",
    "tunnel"
  ],
  "author": "Leask Wong <i@leaskh.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leask/socrates.git"
  },
  "dependencies": {
    "acme-client": "^4.2.5",
    "fast-geoip": "^1.1.66",
    "yargs-parser": "^21.0.1"
  }
}
